{
  "compilerOptions": {
    "outDir": "dist",
    "module": "esnext",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "noEmit": true,
    "pretty": true,
    "jsx": "react",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "baseUrl": "./src",
    "strict": true
  },
  "include": [
    "./src/**/*.*"
  ]
}
